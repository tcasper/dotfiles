{
  "extends": ["atom-haskell-tslint-rules", "tslint-use-atom-disposables"],
  "rules": {
    "align": false,
    "semicolon": false,
    "no-null-keyword": false,
    "no-non-null-assertion": false,
    "ter-no-sparse-arrays": false,
    "space-before-function-paren": false,
    "no-unsafe-any": true,
    "ter-indent": false,
    "no-floating-promises": true
  }
}
